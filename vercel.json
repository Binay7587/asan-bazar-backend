{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "functions": {
      "api/*": {
        "memory": 512,
        "maxDuration": 30
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
}